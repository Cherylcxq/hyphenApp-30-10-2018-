<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="hyphenApp.Views.UserTOSPage">
    <ContentPage.Content>
        <StackLayout BackgroundColor="#fef784" VerticalOptions="FillAndExpand" Spacing="0">

            <!--<custom:TopBar x:Name="topBar" Title="Terms and Conditions" LeftButtonSource="iconblackclose" />-->
            <StackLayout VerticalOptions="FillAndExpand" HorizontalOptions="FillAndExpand" BackgroundColor="White">
                <WebView x:Name="wv" VerticalOptions="FillAndExpand" HorizontalOptions="FillAndExpand" BackgroundColor="White" />
            </StackLayout>
            <Button x:Name="btnCancel" Text="Ok, I've read it." BackgroundColor="#fef784" TextColor="Blue" HeightRequest="60" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>